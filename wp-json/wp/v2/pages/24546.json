	jQuery(".rightVariation").on('click', function(event){
		var varNbr = 0;
		var container = document.getElementById('blog-sidebar-container');
    	var childNodeWidth = jQuery('.single-product-custom-variation:first-child').width();		
		var variationSCrollLeft = 0;
		var AllVariationSCrollLeft = 0;
		//alert(container.scrollLeft);
		if( window.width > 525 ){
			varNbr = 2;
			variationSCrollLeft = container.scrollLeft + childNodeWidth * 2.5;
			AllVariationSCrollLeft = childNodeWidth * varNbr;
		}			
		else{
			varNbr = 0;
			variationSCrollLeft = container.scrollLeft + childNodeWidth * 1.25;
			AllVariationSCrollLeft = (childNodeWidth * 1.25) * varNbr;
		}	
		/*alert(variationSCrollLeft);
		alert(AllVariationSCrollLeft);
		alert(variationSCrollLeft - AllVariationSCrollLeft);*/
		if( (varNbr < 3) || variationSCrollLeft - AllVariationSCrollLeft >= -10 )
			jQuery(".rightVariation").hide();
		jQuery(".leftVariation").show();
	});	
	jQuery(".leftVariationMobile").on('click', function(event){
		//jQuery(".leftVariationMobile").hide();
		jQuery(".rightVariationMobile").show();
	});	
	jQuery(".rightVariationMobile").on('click', function(event){
		var varNbr = 0;
		
		var container = document.getElementById('blog-sidebar-container_2');
