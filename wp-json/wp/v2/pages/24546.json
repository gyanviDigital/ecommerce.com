		var variationSCrollLeft = 0;
		var AllVariationSCrollLeft = 0;
		
		variationSCrollLeft = container.scrollLeft + childNodeWidth * 1.25;
		AllVariationSCrollLeft = (childNodeWidth * 1.25) * varNbr;
		if( varNbr < 3 || variationSCrollLeft - AllVariationSCrollLeft >= -10 )
			jQuery(".rightVariationMobile").hide();
		jQuery(".leftVariationMobile").show();
	});
	
	if(window.screen.width > 525){
		ResiseAllDescription();
	}else{
		ResiseAllDescriptionMobile();
	}	
	function ResiseAllDescription(){
			var haut = 0;
			var cellules = document.querySelectorAll(".variation_descriptions");
			for (var y = 0; y < cellules.length; y++) {
				if(haut < cellules[y].offsetHeight){
					haut = cellules[y].offsetHeight;
				}
			}
			for (var y = 0; y < cellules.length; y++) {
				cellules[y].style.minHeight = haut+"px";
			}
	}
	function ResiseAllDescriptionMobile(){
		for (var i = 1; i < 3; i++) {
			var haut = 0;
			var cellules = document.querySelectorAll(".row_"+i);
			for (var y = 0; y < cellules.length; y++) {
				if(haut < cellules[y].offsetHeight){
					haut = cellules[y].offsetHeight;
				}
			}
			for (var y = 0; y < cellules.length; y++) {
				cellules[y].style.minHeight = haut+"px";
			}
		}
	}
</script>
	

